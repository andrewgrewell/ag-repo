{
  "name": "<%= projectName %> ",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/<%= appName %>/clients/<%= clientName %>",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docusaurus build",
        "cwd": "apps/<%= appName %>/clients/<%= clientName %>"
      },
      "outputs": ["{options.outputPath}"],
      "configurations": {
        "development": {
          "outputPath": "apps/<%= appName %>/clients/<%= clientName %>"
        },
        "production": {}
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docusaurus start",
        "cwd": "apps/<%= appName %>/clients/<%= clientName %>"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/<%= appName %>/clients/<%= clientName %>/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": ["@<%= appName %>", "browser"]
}
