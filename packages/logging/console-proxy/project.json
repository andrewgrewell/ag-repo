{
  "name": "console-proxy",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/logging/console-proxy/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/logging/console-proxy",
        "main": "packages/logging/console-proxy/src/index.ts",
        "tsConfig": "packages/logging/console-proxy/tsconfig.lib.json",
        "assets": ["packages/logging/console-proxy/*.md"]
      }
    },
    "publish": {
      "command": "node tools/scripts/publish.mjs logging-console-proxy {args.ver} {args.tag}",
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/logging/console-proxy/jest.config.ts"
      }
    }
  },
  "tags": ["ag-oss", "js"]
}
