{
  "name": "@ag-oss/source",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "prepare": "git config core.hooksPath .git-hooks",
    "test:all": "nx run-many --target=test --projects=* --parallel=4 -- --runInBand",
    "test:all:coverage": "rm -rf coverage && nx run-many --target=test --projects=* -- --coverage --runInBand --coverageReporters json; ./tools/scripts/merge-coverage-reports.sh",
    "test:affected": "nx affected --target=test --parallel=8",
    "test:affected:coverage": "rm -rf coverage && nx affected --target=test --parallel=4 -- --coverage --coverageReporters json; ./tools/scripts/merge-coverage-reports.sh",
    "build:all": "nx run-many --target=build --projects=*",
    "build:affected": "nx affected --target=build --parallel=8",
    "lint:all": "nx run-many --target=lint --projects=*",
    "lint:affected": "nx affected --target=lint --fix --parallel=8",
    "clear-nx-cache": "rm -rf ./node_modules/.cache/nx",
    "clear-caches": "rm -rf ./.nx/cache",
    "open:coverage": "open $INIT_CWD/coverage/lcov-report/index.html"
  },
  "private": false,
  "devDependencies": {
    "@nx/jest": "^17.2.8",
    "@nx/js": "17.2.8",
    "@nx/workspace": "17.2.8",
    "@testing-library/jest-dom": "^6.1.6",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin": "^1.0.1",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-react-native": "^18.0.0",
    "jest-silent-reporter": "^0.5.0",
    "jsonc-eslint-parser": "^2.4.0",
    "nx": "17.2.8",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "webpack-merge": "^5.10.0"
  }
}
