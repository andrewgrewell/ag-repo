{
  "name": "@ag-oss/source",
  "version": "1.0.0",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": "20.10.0"
  },
  "scripts": {
    "prepare": "git config core.hooksPath .git-hooks",
    "test:all": "nx run-many --target=test --projects=* --parallel=4 -- --runInBand",
    "test:all:coverage": "rm -rf coverage && nx run-many --target=test --projects=* -- --coverage --runInBand --coverageReporters json; ./tools/scripts/merge-coverage-reports.sh",
    "test:affected": "nx affected --target=test --parallel=8",
    "test:affected:coverage": "rm -rf coverage && nx affected --target=test --parallel=4 -- --coverage --coverageReporters json; ./tools/scripts/merge-coverage-reports.sh",
    "build:all": "nx run-many --target=build --projects=*",
    "build:affected": "nx affected --target=build --parallel=8",
    "lint:all": "nx run-many --target=lint --projects=*",
    "lint:affected": "nx affected --target=lint --fix --parallel=8",
    "clear-nx-cache": "rm -rf ./node_modules/.cache/nx",
    "clear-caches": "rm -rf ./.nx/cache",
    "open:coverage": "open $INIT_CWD/coverage/lcov-report/index.html"
  },
  "devDependencies": {
    "@nx/cypress": "^17.2.8",
    "@nx/detox": "^17.2.8",
    "@nx/devkit": "^17.2.8",
    "@nx/eslint": "^17.2.8",
    "@nx/eslint-plugin": "^17.2.8",
    "@nx/expo": "^17.2.8",
    "@nx/jest": "^17.2.8",
    "@nx/js": "17.2.8",
    "@nx/next": "^17.2.8",
    "@nx/plugin": "^17.2.8",
    "@nx/react": "^17.2.8",
    "@nx/workspace": "17.2.8",
    "@swc-node/register": "~1.6.7",
    "@swc/core": "~1.3.85",
    "@testing-library/jest-dom": "^6.1.6",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react": "^14.1.2",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/prompts": "^2.4.9",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "cypress": "^13.6.2",
    "detox": "^20.14.8",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin": "^1.0.1",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-react-native": "^18.0.0",
    "jest-silent-reporter": "^0.5.0",
    "jsonc-eslint-parser": "^2.4.0",
    "nx": "17.2.8",
    "prettier": "^3.1.1",
